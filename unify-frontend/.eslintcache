    })\n      return {savedDests: true}\n    }\n    catch(e){\n      console.log(e.message)\n    }\n  }\n\n  const addDest = (destItem) => {\n    setDest(prevState => ({ ...prevState, ...destItem}))\n  }\n\n  const deleteDest = (destItem) => {\n    let {[destItem] : deleted , ...updatedDest} = dests\n    setDest(updatedDest)\n  }\n\n  return (\n    <useDestContext.Provider value={{dests, getDests, addDest, saveDests, deleteDest}}>\n        {props.children}\n    </useDestContext.Provider>\n  )\n}\n\nexport {useDestContext, DestProvider}","/Users/richardlewis/Documents/githubProjects/unify-epc/unify-frontend/src/components/dest/search.js",[],"/Users/richardlewis/Documents/githubProjects/unify-epc/unify-frontend/src/components/dest/destList.js",["89"],"/Users/richardlewis/Documents/githubProjects/unify-epc/unify-frontend/src/components/dest/map.js",[],"/Users/richardlewis/Documents/githubProjects/unify-epc/unify-frontend/src/components/dest/dest.js",[],"/Users/richardlewis/Documents/githubProjects/unify-epc/unify-frontend/src/components/dest/marker.js",["90"],"/Users/richardlewis/Documents/githubProjects/unify-epc/unify-frontend/src/context/useLocalStore.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":25,"column":6,"nodeType":"97","endLine":25,"endColumn":8,"suggestions":"98"},{"ruleId":"99","severity":1,"message":"100","line":28,"column":13,"nodeType":"101","messageId":"102","endLine":28,"endColumn":16},{"ruleId":"95","severity":1,"message":"103","line":22,"column":8,"nodeType":"97","endLine":22,"endColumn":10,"suggestions":"104"},{"ruleId":"95","severity":1,"message":"105","line":15,"column":5,"nodeType":"97","endLine":15,"endColumn":18,"suggestions":"106"},"no-native-reassign",["107"],"no-negated-in-lhs",["108"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'checkAuth'. Either include it or remove the dependency array.","ArrayExpression",["109"],"no-unused-va